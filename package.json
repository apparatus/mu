{
  "name": "mu",
  "description": "A message based router for building distributed systems",
  "version": "1.0.2",
  "license": "MIT",
  "author": "Peter Elger (https://github.com/pelger)",
  "contributors": [
    "David Mark Clements (https://github.com/davidmarkclements)",
    "Dean McDonnell (https://github.com/mcdonnelldean)"
  ],
  "keywords": [
    "microservices",
    "messaging",
    "router",
    "distributed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/apparatus/mu.git"
  },
  "main": "./core/core.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "lint": "spacey-standard | snazzy",
    "test": "tap test/**/*.test.js --branches=90 --statements=90 --functions=90 --lines=90",
    "check": "npm run test && npm run lint",
    "coverage": "tap test/**/*.test.js --cov --coverage-report=html",
    "coveralls": "tap test/**/*.test.js --cov --coverage-report=text-lcov | coveralls"
  },
  "dependencies": {
    "bloomrun": "^2.3.0",
    "fast-safe-stringify": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "pino": "^2.12.3",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.14",
    "pre-commit": "^1.1.3",
    "snazzy": "^5.0.0",
    "spacey-standard": "^3.0.0",
    "tap": "^7.1.2"
  },
  "pre-commit": [
    "check"
  ]
}
